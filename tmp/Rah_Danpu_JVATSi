  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

-- Dumping data for table `mp75b_formmaker_blocked`

LOCK TABLES `mp75b_formmaker_blocked` WRITE;
UNLOCK TABLES;

-- Table structure for table `mp75b_formmaker_query`

DROP TABLE IF EXISTS `mp75b_formmaker_query`;
CREATE TABLE `mp75b_formmaker_query` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `form_id` int(11) NOT NULL,
  `query` text NOT NULL,
  `details` text NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

